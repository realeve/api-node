{
    "extends": "eslint-config-egg",
    "rules": {
        "indent": [2, 4],
        "array-bracket-spacing": [2, "never"],
        "linebreak-style": [0, "windows"],
        "quotes": 0,
        "semi": [0, "never"],
        "semi-spacing": 0,
        "no-extra-semi": 0,
        "no-unused-vars": [1, { "vars": "all", "args": "none" }],
        "no-trailing-spaces": 2,
        "no-console": 1,
        "strict": 0,
        "eol-last": 0,
        "comma-dangle": "off"
    }
}